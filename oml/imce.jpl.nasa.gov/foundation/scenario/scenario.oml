open terminology <http://imce.jpl.nasa.gov/foundation/scenario/scenario> {

	extends <http://imce.jpl.nasa.gov/foundation/annotation/annotation>

	extends <http://imce.jpl.nasa.gov/foundation/behavior/behavior>

	@rdfs:label="""Scenario"""
	concept Scenario

	@rdfs:label="""Schedulable Constraint"""
	concept SchedulableConstraint

	@rdfs:label="""Temporal Constraint"""
	concept TemporalConstraint

	@rdfs:label="""Time Point"""
	concept TimePoint

	SchedulableConstraint extendsConcept behavior:AttributeConstraint

	SchedulableConstraint extendsConcept TemporalConstraint

	@oml:hasPropertyLabel="""begins at"""
	@oml:hasReificationLabel="""Begins At"""
	reifiedRelationship BeginsAt {
		functional
		unreified = beginsAt
		source = TemporalConstraint
		target = TimePoint }

	@oml:hasPropertyLabel="""constrains"""
	@oml:hasReificationLabel="""Constrains"""
	reifiedRelationship Constrains {
		unreified = constrains
		source = TemporalConstraint
		target = TimePoint }

	@oml:hasPropertyLabel="""ends at"""
	@oml:hasReificationLabel="""Ends At"""
	reifiedRelationship EndsAt {
		functional
		unreified = endsAt
		source = TemporalConstraint
		target = TimePoint }

	@oml:hasPropertyLabel="""incorporates"""
	@oml:hasReificationLabel="""Incorporates"""
	reifiedRelationship Incorporates {
		asymmetric
		irreflexive
		unreified = incorporates
		source = Scenario
		target = TemporalConstraint }

	Scenario extendsAspect analysis:AnalyzedElement

	Scenario extendsAspect analysis:CharacterizedElement

	Scenario extendsAspect base:IdentifiedElement

	TemporalConstraint extendsAspect analysis:AnalyzedElement

	TemporalConstraint extendsAspect analysis:CharacterizedElement

	TemporalConstraint extendsAspect base:IdentifiedElement

	TimePoint extendsAspect analysis:AnalyzedElement

	TimePoint extendsAspect analysis:CharacterizedElement

	TimePoint extendsAspect base:IdentifiedElement

}
